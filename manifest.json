{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Giphy extension for Github",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://github.beeone.at/George/georgefrontend/pull/*"],
